local Roles = {
    ["Sajwaad"] = "ŸÖÿ∑Ÿàÿ±",
    ["ghalidev_0"] = "ŸÖÿ∑Ÿàÿ±",   
    ["Zarba_751"] = "ÿßÿØŸÖŸÜ",    
