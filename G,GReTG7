local selectedAudioID

-- Adicionar uma textbox para inserir o ID do √°udio
Tab:AddTextBox({
    Name = "ÿßÿØÿÆÿßŸÑ ÿµŸàÿ™ ÿßŸÑŸÖŸàÿ≥ŸäŸÇŸâ",
    Description = "ÿßŸÑŸÖŸàÿ≥ŸäŸÇŸâIDÿßŸÉÿ™ÿ® ",
    PlaceholderText = "ÿßÿØÿÆŸÑ ŸÉŸàÿØ ÿßŸÑÿßÿ∫ŸÜŸäÿ©",
    Callback = function(value)
        selectedAudioID = tonumber(value)
    end
})

-- Adicionar uma dropdown para selecionar o √°udio
local audioNames = {}
for _, audio in ipairs(audios) do
    table.insert(audioNames, audio.Name)
end

Tab:AddDropdown({
    Name = "ÿßÿÆÿ™ÿßÿ± ÿµŸàÿ™",
    Description = "ÿßÿÆÿ™Ÿäÿßÿ± ÿµŸàÿ™ ŸÖŸÜ ÿßŸÑŸÇÿßÿ¶ŸÖÿ©",
    Options = audioNames,
    Default = audioNames[1],
    Flag = "selected_audio",
    Callback = function(value)
        for _, audio in ipairs(audios) do
            if audio.Name == value then
                selectedAudioID = audio.ID
                break
            end
        end
    end
})

-- Controle do loop
local audioLoop = false

-- Nova se√ß√£o para loop de √°udio
Tab:AddSection({"ÿ™ŸÉÿ±ÿßÿ± ÿ£Ÿà ÿ≥ÿ®ÿßŸÖ ÿßŸÑÿµŸàÿ™"})

-- Fun√ß√£o para tocar o √°udio repetidamente
local function playLoopedAudio()
    while audioLoop do
        if selectedAudioID then
            local args = {
                [1] = game:GetService("Workspace"),
                [2] = selectedAudioID,
                [3] = 1,
            }
            game:GetService("ReplicatedStorage").RE:FindFirstChild("1Gu1nSound1s"):FireServer(unpack(args))

            -- Criar e tocar o √°udio
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://" .. selectedAudioID
            sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
            sound:Play()
        else
            warn("Nenhum √°udio selecionado!")
        end

        task.wait(0.5) -- Pequeno delay para evitar sobrecarga
    end
end

-- Toggle para loop de √°udio
Tab:AddToggle({
    Name = "ÿ™ŸÅÿπŸäŸÑ ÿßÿ∫ŸÜŸäÿ© ÿ™ŸÑŸÇÿßÿ¶Ÿä",
    Description = "ÿ≥ÿ®ÿßŸÖ ÿßÿ∫ŸÜŸäÿ©",
    Default = false,
    Flag = "audio_loop",
    Callback = function(value)
        audioLoop = value
        if audioLoop then
            task.spawn(playLoopedAudio) -- Inicia o loop em uma nova thread
        end
    end
})

local function playAudio()
    if selectedAudioID then
        local args = {
            [1] = game:GetService("Workspace"),
            [2] = selectedAudioID,
            [3] = 1,
        }
        game:GetService("ReplicatedStorage").RE:FindFirstChild("1Gu1nSound1s"):FireServer(unpack(args))

        -- Criar e tocar o √°udio
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://" .. selectedAudioID
        sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
        sound:Play()
    else
        warn("Nenhum √°udio selecionado!")
    end
end


-- Bot√£o para tocar o √°udio
Tab:AddButton({"ÿ™ÿ¥ÿ∫ŸäŸÑ", function()
    playAudio()
end})

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local audioID = 6314880174 -- ID fixo do √°udio

local function Audio_All_ClientSide(ID)
    local function CheckFolderAudioAll()
        local FolderAudio = workspace:FindFirstChild("Audio all client")
        if not FolderAudio then
            FolderAudio = Instance.new("Folder")
            FolderAudio.Name = "Audio all client"
            FolderAudio.Parent = workspace
        end
        return FolderAudio
    end

    local function CreateSound(ID)
        if type(ID) ~= "number" then
            print("Insira um n√∫mero v√°lido!")
            return nil
        end

        local Folder_Audio = CheckFolderAudioAll()
        if Folder_Audio then
            local Sound = Instance.new("Sound")
            Sound.SoundId = "rbxassetid://" .. ID
            Sound.Volume = 1
            Sound.Looped = false
            Sound.Parent = Folder_Audio
            Sound:Play()
            task.wait(1) -- Tempo de espera antes de remover o som
            Sound:Destroy()
        end
    end

    CreateSound(ID)
end

local function Audio_All_ServerSide(ID)
    if type(ID) ~= "number" then
        print("Insira um n√∫mero v√°lido!")
        return nil
    end

    local GunSoundEvent = ReplicatedStorage:FindFirstChild("1Gu1nSound1s", true)
    if GunSoundEvent then
        GunSoundEvent:FireServer(workspace, ID, 1)
    end
end

-- Toggle para "Estorar ouvido de geral"
Tab:AddToggle({
    Name = "ÿµŸàÿ™ Ÿäÿ¥⁄Ø ÿßŸÑÿßÿ∞ÿßŸÜ",
    Description = "ÿµŸàÿ™ ŸÇŸàŸä ŸÖÿ™ŸÉÿ±ÿ±",
    Default = false,
    Flag = "audio_spam",
    Callback = function(value)
        getgenv().Audio_All_loop_fast = value

        while getgenv().Audio_All_loop_fast do
            Audio_All_ServerSide(audioID)
            task.spawn(function()
                Audio_All_ClientSide(audioID)
            end)
            task.wait(0.03) -- Delay extremamente r√°pido (0.03 segundos)
        end
    end
})

Tab:AddParagraph({"Info", "ÿµŸàÿ™ ŸÑŸÑŸÉŸÑ"})

local TabSp = Window:MakeTab({
    Title = "[EXTRA]ŸÖÿ®ÿßŸÜŸä+ÿ≥ÿ®ÿßŸÖ",
    Icon = "hammer"
})

TabSp:AddSection({ "ÿ™ÿØÿ®ŸäŸÑ/ŸÖÿ®ÿßŸÜŸä" })

local BNumber = 2000 -- Initial default value

TabSp:AddToggle({
    Name = "ÿ™ÿØÿ®ŸäŸÑ ŸÉÿ±ÿßÿ™ ÿ≥ŸÑÿ©",
    Default = false,
    Callback = function(Value)
        BasketToggleH = Value
        if BasketToggleH then
            local Player = game.Players.LocalPlayer
            local Backpack = Player.Backpack
            local Mouse = Player:GetMouse()
            local Character = Player.Character
            local Humanoid = Character:FindFirstChildOfClass("Humanoid")
            local RootPart = Character:FindFirstChild("HumanoidRootPart")
            local OldPosition = RootPart.CFrame
            local Clone = Workspace.WorkspaceCom["001_GiveTools"].Basketball

            for i = 1, BNumber do
                task.wait()
                RootPart.CFrame = Clone.CFrame
                fireclickdetector(Clone.ClickDetector)
            end
            task.wait()
            RootPart.CFrame = OldPosition

            while BasketToggleH do
                task.wait()
                for i, v in ipairs(Character:GetChildren()) do
                    if v.Name == "Basketball" then
                        task.wait()
                        local args = {
                            [1] = Mouse.Hit.p
                        }
                        v.ClickEvent:FireServer(unpack(args))
                    end
                end
            end
        end
    end
})

TabSp:AddSlider({
    Name = "ŸÉŸÖŸäÿ© ŸÖÿ±ÿßÿ™ ÿßŸÑÿ≥ŸÑÿ©",
    MinValue = 1,
    MaxValue = 2000, -- Adjust as needed
    Default = BNumber,
    Increase = 1,
    Callback = function(Value)
        BNumber = Value
    end
})

local duplicarAtivo = false
local duplicarThread = nil

TabSp:AddToggle({
    Name = "ÿßÿÆÿ∞ ŸÇŸÜÿßÿ®ŸÑ",
    Default = false,
    Callback = function(v)
        duplicarAtivo = v
        if duplicarAtivo then
            duplicarThread = task.spawn(function()
                local Player = game.Players.LocalPlayer
                local Character = Player.Character or Player.CharacterAdded:Wait()
                local RootPart = Character:WaitForChild("HumanoidRootPart")
                local OldPos = RootPart.CFrame

                local Bomb = game:GetService("Workspace"):FindFirstChild("WorkspaceCom")
                    and game:GetService("Workspace").WorkspaceCom["001_CriminalWeapons"]:FindFirstChild("GiveTools")
                    and game:GetService("Workspace").WorkspaceCom["001_CriminalWeapons"].GiveTools:FindFirstChild("Bomb")

                if not Bomb or not Bomb:FindFirstChild("ClickDetector") then
                    warn("Erro ao encontrar a Bomb! Tente novamente.")
                    duplicarAtivo = false
                    return
                end

                while duplicarAtivo do
                    for i = 1, 12 do
                        RootPart.CFrame = Bomb.CFrame
                        fireclickdetector(Bomb.ClickDetector)
                        task.wait(0)
                    end
                    task.wait(0.001)
                end

                -- Quando desativar, volta para a posi√ß√£o original
                RootPart.CFrame = OldPos
            end)
        end
    end
})

local spam = false
local playerName = game.Players.LocalPlayer.Name -- pega automaticamente o nome do player local

TabSp:AddToggle({
    Name = "ÿ≥ÿ®ÿßŸÖ ÿ™ŸÅÿ¨Ÿäÿ± ŸÇŸÜÿßÿ®ŸÑ",
    Default = false,
    Callback = function(v)
        spam = v
        if spam then
            task.spawn(function()
                while spam do
                    local args = {
                        [1] = "Bomb" .. playerName
                    }
                    game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1Blo1wBomb1sServe1r"):FireServer(unpack(args))
                    task.wait(0.1) -- intervalo de 0.1 segundo entre cada spam
                end
            end)
        end
    end
})


TabSp:AddSection({"ÿßŸÑŸÖÿ®ÿßŸÜŸä"})

local function organizarItensEmCuboAcima()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local backpack = player.Backpack
    local rootPart = character:WaitForChild("HumanoidRootPart")

    -- ÿ¨ŸÖÿπ ÿ¨ŸÖŸäÿπ ÿ£ÿØŸàÿßÿ™ FireX ŸÖŸÜ ÿßŸÑÿ¥ÿÆÿµŸäÿ© ŸàÿßŸÑŸÖÿÆÿ≤ŸÜ
    local itens = {}

    for _, item in pairs(character:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            table.insert(itens, item)
        end
    end

    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            table.insert(itens, item)
        end
    end

    -- ÿßŸÑÿ™ÿ≠ŸÇŸÇ ŸÖŸÜ Ÿàÿ¨ŸàÿØ ÿ£ÿØŸàÿßÿ™
    if #itens == 0 then
        warn("ŸÑÿß ÿ™Ÿàÿ¨ÿØ ÿ£ÿØŸàÿßÿ™ FireX ŸÑŸÑÿ™ŸÜÿ∏ŸäŸÖ.")
        return
    end

    -- ŸÖÿ±ŸÉÿ≤ ÿßŸÑŸÖŸÉÿπÿ®: ŸÅŸàŸÇ ÿßŸÑÿ±ÿ£ÿ≥
    local head = character:FindFirstChild("Head") or rootPart
    local center = head.Position + Vector3.new(0, 15, 0) -- 15 Ÿàÿ≠ÿØÿ© ŸÅŸàŸÇ ÿßŸÑÿ±ÿ£ÿ≥

    -- ÿ£ÿ®ÿπÿßÿØ ÿßŸÑŸÖŸÉÿπÿ® ÿßŸÑÿπŸÖŸÑÿßŸÇ
    local cubeSize = 30 -- 30 √ó 30 √ó 30 Ÿàÿ≠ÿØÿ© (ŸÖŸÉÿπÿ® ÿ∂ÿÆŸÖ)

    -- ÿπÿØÿØ ÿßŸÑŸÜŸÇÿßÿ∑ ŸÅŸä ŸÉŸÑ ÿßÿ™ÿ¨ÿßŸá
    local steps = 15 -- ŸÜŸÇÿ≥ŸÖ ÿßŸÑŸÖŸÉÿπÿ® ÿ•ŸÑŸâ 15√ó15√ó15 = 3375 ŸÜŸÇÿ∑ÿ© ŸÉÿ≠ÿØ ÿ£ŸÇÿµŸâ

    -- ÿßŸÑÿ™ÿ®ÿßÿπÿØ ÿ®ŸäŸÜ ŸÉŸÑ ŸÜŸÇÿ∑ÿ©
    local spacing = cubeSize / steps

    -- ÿ™Ÿàÿ≤Ÿäÿπ ÿßŸÑÿ£ÿØŸàÿßÿ™ ŸÅŸä ÿ¥ÿ®ŸÉÿ© ŸÖŸÉÿπÿ®ÿ© ÿ´ŸÑÿßÿ´Ÿäÿ© ÿßŸÑÿ£ÿ®ÿπÿßÿØ
    local count = 1
    for ix = 0, steps - 1 do
        for iy = 0, steps - 1 do
            for iz = 0, steps - 1 do
                if count > #itens then break end

                -- ÿ≠ÿ≥ÿßÿ® ÿßŸÑŸÖŸàÿ∂ÿπ ÿßŸÑŸÜÿ≥ÿ®Ÿä
                local x = (ix - steps/2) * spacing
                local y = (iy - steps/2) * spacing
                local z = (iz - steps/2) * spacing

                local pos = center + Vector3.new(x, y, z)

                -- ÿ™ÿ¨ŸáŸäÿ≤ ÿßŸÑÿ£ÿØÿßÿ©
                local item = itens[count]
                item.Parent = character
                item.GripPos = pos - rootPart.Position

                task.wait(0.001) -- ÿ™ÿ£ÿÆŸäÿ± ÿ®ÿ≥Ÿäÿ∑ ŸÑÿ™ÿ¨ŸÜÿ® ÿßŸÑÿ™ÿ¨ŸÖŸäÿØ

                -- ÿ•ÿπÿßÿØÿ© ÿßŸÑÿ£ÿØÿßÿ© ÿ•ŸÑŸâ ÿßŸÑŸÖÿÆÿ≤ŸÜ
                item.Parent = backpack

                count = count + 1
            end
        end
    end

    -- ÿ™ÿ¨ŸáŸäÿ≤ ÿ¨ŸÖŸäÿπ ÿ£ÿØŸàÿßÿ™ FireX ŸÖÿ±ÿ© ÿ£ÿÆŸäÿ±ÿ© (ŸÑÿ™ÿ±Ÿâ ÿßŸÑŸÖŸÉÿπÿ®)
    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            item.Parent = character
        end
    end

    -- ÿ•ÿ¥ÿπÿßÿ± ÿ®ÿßŸÑŸÜÿ¨ÿßÿ≠
    print("ÿ™ŸÖ ÿ•ŸÜÿ¥ÿßÿ° ŸÖŸÉÿπÿ® ÿπŸÖŸÑÿßŸÇ ŸÖŸÜ " .. (count - 1) .. " ÿ£ÿØÿßÿ© FireX!")
end

TabSp:AddButton({
    Name = "üî• FireX: ŸÖŸÉÿπÿ® ÿπŸÖŸÑÿßŸÇ ŸÅŸàŸÇ ÿßŸÑÿ±ÿ£ÿ≥",
    Callback = function()
        local Player = game.Players.LocalPlayer 
        local Character = Player.Character or Player.CharacterAdded:Wait()
        local RootPart = Character:WaitForChild("HumanoidRootPart")
        
        -- ÿßŸÑÿ®ÿ≠ÿ´ ÿπŸÜ ÿßŸÑŸÉÿßÿ¶ŸÜ
        local Clone = game:GetService("Workspace"):FindFirstChild("WorkspaceCom") 
            and game:GetService("Workspace").WorkspaceCom:FindFirstChild("001_GiveTools")
            and game:GetService("Workspace").WorkspaceCom["001_GiveTools"]:FindFirstChild("FireX")

        if Clone and Clone:FindFirstChild("ClickDetector") then
            local OldPos = RootPart.CFrame

            -- ÿ¨ŸÖÿπ 2000 ÿ£ÿØÿßÿ© (ÿ£Ÿà ÿ£ŸÉÿ´ÿ± ÿ•ÿ∞ÿß ÿ£ŸÖŸÉŸÜ)
            for i = 1, 2000 do
                RootPart.CFrame = Clone.CFrame
                pcall(fireclickdetector, Clone.ClickDetector) -- ÿ¢ŸÖŸÜ ÿ∂ÿØ ÿßŸÑÿ£ÿÆÿ∑ÿßÿ°
                task.wait(0.005) -- ÿ™ÿ£ÿÆŸäÿ± ÿ®ÿ≥Ÿäÿ∑
            end

            -- ÿßŸÑÿπŸàÿØÿ© ÿ•ŸÑŸâ ÿßŸÑŸÖŸÉÿßŸÜ ÿßŸÑÿ£ÿµŸÑŸä
            RootPart.CFrame = OldPos

            -- ÿ™ŸÜŸÅŸäÿ∞ ÿ™ŸÜÿ∏ŸäŸÖ ÿßŸÑŸÖŸÉÿπÿ®
            organizarItensEmCuboAcima()
        else
            warn("FireX ÿ£Ÿà ClickDetector ÿ∫Ÿäÿ± ŸÖŸàÿ¨ŸàÿØ!")
            Tab:AddNotification({
                Name = "ÿÆÿ∑ÿ£",
                Content = "ŸÑŸÖ Ÿäÿ™ŸÖ ÿßŸÑÿπÿ´Ÿàÿ± ÿπŸÑŸâ FireX!",
                Time = 5
            })
        end
    end
})

-- ÿØÿßŸÑÿ© ŸÑÿ™ŸÜÿ∏ŸäŸÖ ÿßŸÑÿ£ÿØŸàÿßÿ™ ŸÅŸä ÿ¥ŸÉŸÑ ŸÉÿ±ÿ© ŸÅŸàŸÇ ÿßŸÑÿ±ÿ£ÿ≥
local function organizarItensEmBolaAcima()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local backpack = player.Backpack
    local rootPart = character:WaitForChild("HumanoidRootPart")

    -- ÿ¨ŸÖÿπ ÿ¨ŸÖŸäÿπ ÿ£ÿØŸàÿßÿ™ FireX ŸÖŸÜ ÿßŸÑÿ¥ÿÆÿµŸäÿ© ŸàÿßŸÑŸÖÿÆÿ≤ŸÜ
    local itens = {}

    for _, item in pairs(character:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            table.insert(itens, item)
        end
    end

    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            table.insert(itens, item)
        end
    end

    -- ÿßŸÑÿ™ÿ≠ŸÇŸÇ ŸÖŸÜ Ÿàÿ¨ŸàÿØ ÿ£ÿØŸàÿßÿ™
    if #itens == 0 then
        warn("ŸÑÿß ÿ™Ÿàÿ¨ÿØ ÿ£ÿØŸàÿßÿ™ FireX ŸÑŸÑÿ™ŸÜÿ∏ŸäŸÖ.")
        return
    end

    -- ŸÖÿ±ŸÉÿ≤ ÿßŸÑŸÉÿ±ÿ©: ŸÅŸàŸÇ ÿßŸÑÿ±ÿ£ÿ≥ (ŸÜÿ≥ÿ™ÿÆÿØŸÖ ÿßŸÑÿ±ÿ£ÿ≥ ÿ£Ÿà HumanoidRootPart)
    local head = character:FindFirstChild("Head") or rootPart
    local center = head.Position + Vector3.new(0, -5, 0) -- 5 Ÿàÿ≠ÿØÿßÿ™ ŸÅŸàŸÇ ÿßŸÑÿ±ÿ£ÿ≥

    -- ŸÜÿµŸÅ ŸÇÿ∑ÿ± ÿßŸÑŸÉÿ±ÿ©
    local radius = 8

    -- ÿ≤ÿßŸàŸäÿ© ÿπÿ¥Ÿàÿßÿ¶Ÿäÿ© ŸÑÿ™Ÿàÿ≤Ÿäÿπ ŸÖÿ™ÿ≥ÿßŸàŸç ÿ™ŸÇÿ±Ÿäÿ®Ÿãÿß (Spherical Distribution)
    for i, item in pairs(itens) do
        -- ÿ≤ŸàÿßŸäÿß ÿπÿ¥Ÿàÿßÿ¶Ÿäÿ©
        local phi = math.acos(1 - 2 * (i / (#itens + 1))) -- ÿ™Ÿàÿ≤Ÿäÿπ ŸÖÿ™ÿ≥ÿßŸàŸç ÿπŸÑŸâ ÿßŸÑŸÖÿ≠Ÿàÿ± Y
        local theta = 7.777 * i -- ÿ±ŸÇŸÖ ÿ∫Ÿäÿ± ŸÖŸÜÿ∑ŸÇŸä ŸäŸÇŸÑŸÑ ŸÖŸÜ ÿßŸÑÿ™ŸÉÿ±ÿßÿ± (ŸÖÿ´ŸÑ 7.777)

        -- ÿßŸÑÿ™ÿ≠ŸàŸäŸÑ ÿ•ŸÑŸâ ÿ•ÿ≠ÿØÿßÿ´Ÿäÿßÿ™ ŸÉÿ±ŸàŸäÿ©
        local x = math.cos(theta) * math.sin(phi)
        local y = math.cos(phi)
        local z = math.sin(theta) * math.sin(phi)

        -- ÿ™ÿ≠ÿØŸäÿØ ÿßŸÑŸÖŸàÿ∂ÿπ ÿπŸÑŸâ ÿ≥ÿ∑ÿ≠ ÿßŸÑŸÉÿ±ÿ©
        local pos = center + Vector3.new(x, y, z) * radius

        -- ÿ™ÿ¨ŸáŸäÿ≤ ÿßŸÑÿ£ÿØÿßÿ© ŸÑÿ™ÿπÿØŸäŸÑ GripPos
        item.Parent = character

        -- ÿ∂ÿ®ÿ∑ GripPos ÿ®ÿ≠Ÿäÿ´ ÿ™ÿ∏Ÿáÿ± ÿßŸÑÿ£ÿØÿßÿ© ŸÅŸä ÿßŸÑŸÖŸàÿ∂ÿπ ÿßŸÑŸÖÿ∑ŸÑŸàÿ®
        item.GripPos = pos - rootPart.Position

        task.wait(0.01) -- ÿ™ÿ£ÿÆŸäÿ± ÿ®ÿ≥Ÿäÿ∑ ŸÑÿ±ÿ§Ÿäÿ© ÿßŸÑÿ™ÿ£ÿ´Ÿäÿ±

        -- ÿ•ÿπÿßÿØÿ© ÿßŸÑÿ£ÿØÿßÿ© ÿ•ŸÑŸâ ÿßŸÑŸÖÿÆÿ≤ŸÜ (ÿ≠ÿ™Ÿâ ŸÑÿß ÿ™ÿπŸäŸÇ ÿßŸÑŸÑÿßÿπÿ®)
        item.Parent = backpack
    end

    -- ÿ™ÿ¨ŸáŸäÿ≤ ÿ¨ŸÖŸäÿπ ÿ£ÿØŸàÿßÿ™ FireX ŸÖÿ±ÿ© ÿ£ÿÆŸäÿ±ÿ© (ŸÑÿ™ÿ±Ÿâ ÿßŸÑŸÉÿ±ÿ©)
    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            item.Parent = character
        end
    end
end

TabSp:AddButton({
    Name = "Firex ŸÉÿ±ÿ© ÿπŸÖŸÑÿßŸÇÿ© ŸÅŸàŸÇ ÿßŸÑÿ±ÿ£ÿ≥",
    Callback = function()
        local Player = game.Players.LocalPlayer 
        local Character = Player.Character or Player.CharacterAdded:Wait()
        local RootPart = Character:WaitForChild("HumanoidRootPart")
        
        -- ÿßŸÑÿ®ÿ≠ÿ´ ÿπŸÜ ÿßŸÑŸÉÿßÿ¶ŸÜ
        local Clone = game:GetService("Workspace"):FindFirstChild("WorkspaceCom") 
            and game:GetService("Workspace").WorkspaceCom:FindFirstChild("001_GiveTools")
            and game:GetService("Workspace").WorkspaceCom["001_GiveTools"]:FindFirstChild("FireX")

        if Clone and Clone:FindFirstChild("ClickDetector") then
            local OldPos = RootPart.CFrame

            -- ÿ¨ŸÖÿπ 600 ÿ£ÿØÿßÿ©
            for i = 1, 600 do
                RootPart.CFrame = Clone.CFrame
                fireclickdetector(Clone.ClickDetector)
                task.wait()
            end

            -- ÿßŸÑÿπŸàÿØÿ©
            RootPart.CFrame = OldPos

            -- ÿ™ŸÜŸÅŸäÿ∞ ÿ™ŸÜÿ∏ŸäŸÖ ÿßŸÑŸÉÿ±ÿ©
            organizarItensEmBolaAcima()
        else
            warn("FireX ÿ£Ÿà ClickDetector ÿ∫Ÿäÿ± ŸÖŸàÿ¨ŸàÿØ!")
        end
    end
})

local BNumber = 600 -- Valor fixo
local distanciaEntreItens = 3 -- Dist√É¬¢ncia entre os itens ao longo do bast√É¬£o

-- Fun√É¬ß√É¬£o para organizar itens em forma de bast√É¬£o gigante para baixo
local function organizarItensEmBastaoParaBaixo()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local backpack = player.Backpack
    local rootPart = character:WaitForChild("HumanoidRootPart")

    -- Coleta todos os Tools no personagem e na mochila
    local itens = {}

    -- Procura Tools no personagem
    for _, item in pairs(character:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            table.insert(itens, item)
        end
    end

    -- Procura Tools na mochila
    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            table.insert(itens, item)
        end
    end

    -- Verifica duplicatas e organiza por nome
    local duplicatas = {}
    for _, item in pairs(itens) do
        if not duplicatas[item.Name] then
            duplicatas[item.Name] = {}
        end
        table.insert(duplicatas[item.Name], item)
    end

    -- Para cada grupo de duplicatas, organize-os em formato de bast√É¬£o
    local yOffset = 0 -- Offset inicial no eixo Y
    for _, grupo in pairs(duplicatas) do
        local total = #grupo
        if total > 1 then
            for i, item in pairs(grupo) do
                -- Equipa o item para ajustar o GripPos
                item.Parent = character

                -- Define a posi√É¬ß√É¬£o vertical para o item no bast√É¬£o (para baixo no eixo Y)
                item.GripPos = Vector3.new(0, -yOffset, 0)

                -- Incrementa o offset no eixo Y para "descer"
                yOffset = yOffset + distanciaEntreItens

                -- Aguarda um pouco para o efeito ser vis√É¬≠vel
                task.wait()

                -- Solta o item colocando de volta na mochila
                item.Parent = backpack
            end
        end
    end

    -- Equipa todos os itens "FireX" da mochila
    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            item.Parent = character
        end
    end
end


TabSp:AddButton({
  Name = "ÿπÿµÿß ÿ∑ŸàŸäŸÑÿ© ÿ∑ŸÅÿßŸäÿ© ÿ≠ÿ±ŸäŸÇ[FireX]",
  Callback = function()
    local Player = game.Players.LocalPlayer
        local Character = Player.Character or Player.CharacterAdded:Wait()
        local RootPart = Character:WaitForChild("HumanoidRootPart")

        -- Atualiza√É¬ß√É¬£o da hierarquia para FireX
        local Clone = game:GetService("Workspace"):FindFirstChild("WorkspaceCom")
            and game:GetService("Workspace").WorkspaceCom:FindFirstChild("001_GiveTools")
            and game:GetService("Workspace").WorkspaceCom["001_GiveTools"]:FindFirstChild("FireX")

        if Clone and Clone:FindFirstChild("ClickDetector") then
            local OldPos = RootPart.CFrame -- Armazena a posi√É¬ß√É¬£o original

            -- Loop para pegar os itens repetidamente
            local processed = 0
            while processed < BNumber do
                RootPart.CFrame = Clone.CFrame -- Teletransporta para o clone
                fireclickdetector(Clone.ClickDetector) -- Clica no detector
                processed = processed + 1
                task.wait() -- Pausa para evitar travamento
            end

            -- Retorna √É¬† posi√É¬ß√É¬£o original
            RootPart.CFrame = OldPos

            -- Executa a fun√É¬ß√É¬£o de organiza√É¬ß√É¬£o ao final do loop
            organizarItensEmBastaoParaBaixo()
        else
            warn("FireX ou ClickDetector n√É¬£o encontrado!")
        end
  end
})


local BNumber = 600 -- Valor fixo
local raioCirculo = 5 -- Dist√É¬¢ncia do c√É¬≠rculo em rela√É¬ß√É¬£o ao personagem

-- Fun√É¬ß√É¬£o para organizar itens em formato de c√É¬≠rculo
local function organizarItensEmCirculo()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local backpack = player.Backpack
local rootPart = character:WaitForChild("HumanoidRootPart")

-- Coleta todos os Tools no personagem e na mochila  
local itens = {}  
  
-- Procura Tools no personagem  
for _, item in pairs(character:GetChildren()) do  
    if item:IsA("Tool") and item.Name == "FireX" then  
        table.insert(itens, item)  
    end  
end  
  
-- Procura Tools na mochila  
for _, item in pairs(backpack:GetChildren()) do  
    if item:IsA("Tool") and item.Name == "FireX" then  
        table.insert(itens, item)  
    end  
end  
  
-- Verifica duplicatas e organiza por nome  
local duplicatas = {}  
for _, item in pairs(itens) do  
    if not duplicatas[item.Name] then  
        duplicatas[item.Name] = {}  
    end  
    table.insert(duplicatas[item.Name], item)  
end  
  
-- Para cada grupo de duplicatas, organize-os em formato de c√É¬≠rculo  
for _, grupo in pairs(duplicatas) do  
    local total = #grupo  
    if total > 1 then  
        for i, item in pairs(grupo) do  
            -- Equipa o item para ajustar o GripPos  
            item.Parent = character  
              
            -- Calcula o √É¬¢ngulo de cada item no c√É¬≠rculo  
            local angulo = (i / total) * (2 * math.pi)  
              
            -- Calcula a posi√É¬ß√É¬£o circular atr√É¬°s do jogador  
            local offsetX = math.cos(angulo) * raioCirculo  
            local offsetZ = math.sin(angulo) * raioCirculo  
              
            -- Define a posi√É¬ß√É¬£o atr√É¬°s do HumanoidRootPart  
            item.GripPos = Vector3.new(-offsetX, 0, -offsetZ - raioCirculo)  
              
            -- Aguarda um pouco para o efeito ser vis√É¬≠vel  
            task.wait()  
              
            -- Solta o item colocando de volta na mochila  
            item.Parent = backpack  
        end  
    end  
end  
  
-- Equipa todos os itens "FireX" da mochila  
for _, item in pairs(backpack:GetChildren()) do  
    if item:IsA("Tool") and item.Name == "FireX" then  
        item.Parent = character  
    end  
end

end


TabSp:AddButton({
  Name = "[FireX]ÿØÿßÿ¶ÿ±ÿ©",
  Callback = function()
    local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local RootPart = Character:WaitForChild("HumanoidRootPart")

-- Atualiza√É¬ß√É¬£o da hierarquia para FireX  
    local Clone = game:GetService("Workspace"):FindFirstChild("WorkspaceCom")   
        and game:GetService("Workspace").WorkspaceCom:FindFirstChild("001_GiveTools")  
        and game:GetService("Workspace").WorkspaceCom["001_GiveTools"]:FindFirstChild("FireX")  

    if Clone and Clone:FindFirstChild("ClickDetector") then  
        local OldPos = RootPart.CFrame -- Armazena a posi√É¬ß√É¬£o original  

        -- Loop para pegar os itens repetidamente  
        local processed = 0  
        while processed < BNumber do  
            RootPart.CFrame = Clone.CFrame -- Teletransporta para o clone  
            fireclickdetector(Clone.ClickDetector) -- Clica no detector  
            processed = processed + 1  
            task.wait() -- Pausa para evitar travamento  
        end  

        -- Retorna √É¬† posi√É¬ß√É¬£o original  
        RootPart.CFrame = OldPos   

        -- Executa a fun√É¬ß√É¬£o de organiza√É¬ß√É¬£o ao final do loop  
        organizarItensEmCirculo()  
    else  
        warn("FireX ou ClickDetector n√É¬£o encontrado!")  
    end  
  end
})


local BNumber = 600 -- Valor fixo
local distanciaAtras = 5 -- Dist√É¬¢ncia da parede em rela√É¬ß√É¬£o ao personagem
local espacamentoX = 3 -- Espa√É¬ßamento horizontal entre itens
local espacamentoY = 3 -- Espa√É¬ßamento vertical entre itens

-- Fun√É¬ß√É¬£o para organizar itens em formato de parede
local function organizarItensEmParede()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local backpack = player.Backpack
    local rootPart = character:WaitForChild("HumanoidRootPart")

    -- Coleta todos os Tools no personagem e na mochila
    local itens = {}
    
    -- Procura Tools no personagem
    for _, item in pairs(character:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            table.insert(itens, item)
        end
    end
    
    -- Procura Tools na mochila
    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            table.insert(itens, item)
        end
    end
    
    -- Verifica duplicatas e organiza por nome
    local duplicatas = {}
    for _, item in pairs(itens) do
        if not duplicatas[item.Name] then
            duplicatas[item.Name] = {}
        end
        table.insert(duplicatas[item.Name], item)
    end
    
    -- Organiza os itens em formato de parede
    for _, grupo in pairs(duplicatas) do
        local total = #grupo
        
        -- Calcula o n√É¬∫mero de colunas e linhas necess√É¬°rias
        local colunas = math.ceil(math.sqrt(total))
        local linhas = math.ceil(total / colunas)
        
        for i, item in pairs(grupo) do
            -- Calcula a posi√É¬ß√É¬£o na grade
            local linha = math.floor((i-1) / colunas)
            local coluna = (i-1) % colunas
            
            -- Equipa o item para ajustar o GripPos
            item.Parent = character
            
            -- Ajusta a posi√É¬ß√É¬£o na parede atr√É¬°s do jogador
            item.GripPos = Vector3.new(coluna * espacamentoX, -linha * espacamentoY, -distanciaAtras)
            
            -- Aguarda um pouco para o efeito ser vis√É¬≠vel
            task.wait()
            
            -- Solta o item colocando de volta na mochila
            item.Parent = backpack
        end
    end
    
    -- Equipa todos os itens "FireX" da mochila
    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            item.Parent = character
        end
    end
end


TabSp:AddButton({
  Name = "[Firex]ÿ¨ÿØÿßÿ± ÿπŸÖŸÑÿßŸÇ",
  Callback = function()
    local Player = game.Players.LocalPlayer 
        local Character = Player.Character or Player.CharacterAdded:Wait()
        local RootPart = Character:WaitForChild("HumanoidRootPart")
        
        -- Atualiza√É¬ß√É¬£o da hierarquia para FireX
        local Clone = game:GetService("Workspace"):FindFirstChild("WorkspaceCom") 
            and game:GetService("Workspace").WorkspaceCom:FindFirstChild("001_GiveTools")
            and game:GetService("Workspace").WorkspaceCom["001_GiveTools"]:FindFirstChild("FireX")

        if Clone and Clone:FindFirstChild("ClickDetector") then
            local OldPos = RootPart.CFrame -- Armazena a posi√É¬ß√É¬£o original

            -- Loop para pegar os itens repetidamente
            local processed = 0
            while processed < BNumber do
                RootPart.CFrame = Clone.CFrame -- Teletransporta para o clone
                fireclickdetector(Clone.ClickDetector) -- Clica no detector
                processed = processed + 1
                task.wait() -- Pausa para evitar travamento
            end

            -- Retorna √É¬† posi√É¬ß√É¬£o original
            RootPart.CFrame = OldPos 

            -- Executa a fun√É¬ß√É¬£o de organiza√É¬ß√É¬£o ao final do loop
            organizarItensEmParede()
        else
            warn("FireX ou ClickDetector n√É¬£o encontrado!")
        end
  end
})

