-- ==================== ŸÇÿ≥ŸÖ: ŸÑÿßŸÇ ÿ®ÿßŸÑŸÇŸÜÿßÿ®ŸÑ ====================
Tablag:AddSection("ŸÑÿßŸÇ ŸÇŸÜÿßÿ®ŸÑ")

Tablag:AddToggle({
    Name = "ŸÑÿßŸÇ ŸÇŸÜÿßÿ®ŸÑ",
    Default = false,
    Callback = function(state)
        LagToggles.Bomb = state
        if state then
            local Player = game.Players.LocalPlayer
            local Character = Player.Character or Player.CharacterAdded:Wait()
            local RootPart = Character:WaitForChild("HumanoidRootPart")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")

            local Bomb = workspace:FindFirstChild("WorkspaceCom")
                and workspace.WorkspaceCom:FindFirstChild("001_CriminalWeapons")
                and workspace.WorkspaceCom["001_CriminalWeapons"]:FindFirstChild("GiveTools")
                and workspace.WorkspaceCom["001_CriminalWeapons"].GiveTools:FindFirstChild("Bomb")

            if not Bomb or not Bomb:FindFirstChild("ClickDetector") then
                Tablag:AddNotification({
                    Name = "ÿÆÿ∑ÿ£",
                    Content = "ÿßŸÑŸÇŸÜÿ®ŸÑÿ© ÿ∫Ÿäÿ± ŸÖŸàÿ¨ŸàÿØÿ©!",
                    Time = 5
                })
                return
            end

            -- ÿ≠ŸÑŸÇÿ© ÿßŸÑŸÜŸÇÿ±
            task.spawn(function()
                while LagToggles.Bomb and Bomb and RootPart do
                    pcall(function()
                        RootPart.CFrame = Bomb.CFrame
                        fireclickdetector(Bomb.ClickDetector)
                    end)
                    task.wait(0.0001)
                end
            end)

            -- ÿ≠ŸÑŸÇÿ© ÿ•ÿ±ÿ≥ÿßŸÑ ÿßŸÑŸÇŸÜÿ®ŸÑÿ©
            task.spawn(function()
                while LagToggles.Bomb do
                    pcall(function()
                        local VirtualInputManager = game:GetService("VirtualInputManager")
                        VirtualInputManager:SendMouseButtonEvent(500, 500, 0, true, game, 0)
                        task.wait(1.5)
                        VirtualInputManager:SendMouseButtonEvent(500, 500, 0, false, game, 0)

                        local args = { [1] = "Bomb" .. Player.Name }
                        local BombServer = ReplicatedStorage:FindFirstChild("RE"):FindFirstChild("1Blo1wBomb1sServe1r")
                        if BombServer then
                            BombServer:FireServer(unpack(args))
                        end
                    end)
                    task.wait(1.5)
                end
            end)
        end
    end
})

Tablag:AddParagraph({
    Title = "ŸÖŸÑÿßÿ≠ÿ∏ÿ©",
    Text = "ÿßŸÜÿ™ÿ∏ÿ± 35 ÿ´ÿßŸÜŸäÿ© ŸÑÿ∏ŸáŸàÿ± ÿ™ÿ£ÿ´Ÿäÿ± ÿßŸÑŸÑÿßŸÇ"
})
local RedzTab = Window:MakeTab({
    Title = "(EXTRA)ÿßŸÑÿ≠ŸÖÿßŸäÿ©",
    Icon = "shield"
})

local voidProtectionEnabled = false

local function ActivateVoidProtection()
    voidProtectionEnabled = true
    game.Workspace.FallenPartsDestroyHeight = 0/0
end

local function DeactivateVoidProtection()
    voidProtectionEnabled = false
    game.Workspace.FallenPartsDestroyHeight = -500
end

RedzTab:AddToggle({
    Name = "ŸÖÿ∂ÿßÿØ ŸÖŸàÿ™(ŸÖÿØŸÖÿ±üíÄ)",
    Default = false,
    Callback = function(state)
        if state then
            ActivateVoidProtection()
        else
            DeactivateVoidProtection()
        end
    end
})
