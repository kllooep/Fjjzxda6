local LagToggles = { Laptop = false, Phone = false, Bomb = false }

-- ÿØÿßŸÑÿ© ÿßŸÑŸÜŸÇÿ± ÿßŸÑÿπÿßŸÖÿ©
local function clickNormally(object)
    local clickDetector = object:FindFirstChildWhichIsA("ClickDetector")
    if clickDetector then
        pcall(fireclickdetector, clickDetector)
    end
end

-- ==================== ŸÇÿ≥ŸÖ: ŸÑÿßŸÇ ÿ®ÿßŸÑÿßÿ®ÿ™Ÿàÿ® ====================
Tablag:AddSection("ŸÑÿßŸÇ ÿ®ÿßŸÑÿßÿ®ÿ™Ÿàÿ®")

Tablag:AddToggle({
    Name = "ŸÑÿßŸÇ ÿ®ÿßŸÑÿßÿ®ÿ™Ÿàÿ®",
    Default = false,
    Callback = function(state)
        LagToggles.Laptop = state
        if state then
            local laptopPath = workspace:FindFirstChild("WorkspaceCom")
                and workspace.WorkspaceCom:FindFirstChild("001_GiveTools")
                and workspace.WorkspaceCom["001_GiveTools"]:FindFirstChild("Laptop")

            if not laptopPath then
                Tablag:AddNotification({
                    Name = "ÿÆÿ∑ÿ£",
                    Content = "ÿßŸÑÿßÿ®ÿ™Ÿàÿ® ÿ∫Ÿäÿ± ŸÖŸàÿ¨ŸàÿØ!",
                    Time = 5
                })
                return
            end

            -- ÿ™ÿ¥ÿ∫ŸäŸÑ ÿßŸÑÿ≠ŸÑŸÇÿ© ŸÅŸä ÿÆŸäÿ∑ ŸÖŸÜŸÅÿµŸÑ
            task.spawn(function()
                local teleportCount = 0
                while LagToggles.Laptop and laptopPath do
                    local char = game.Players.LocalPlayer.Character
                    if char and char:FindFirstChild("HumanoidRootPart") then
                        char.HumanoidRootPart.CFrame = laptopPath.CFrame
                        clickNormally(laptopPath)
                    end
                    teleportCount += 1
                    if teleportCount % 100 == 0 then task.wait(0.1) end -- ÿ™ÿ¨ŸÜÿ® ÿßŸÑÿ™ÿ¨ŸÖŸäÿØ
                end
            end)
        end
    end
})

Tablag:AddParagraph({
    Title = "ŸÖŸÑÿßÿ≠ÿ∏ÿ©",
    Text = "ÿßŸÜÿ™ÿ∏ÿ± 20 ÿ´ÿßŸÜŸäÿ© ŸÑÿ∏ŸáŸàÿ± ÿ™ÿ£ÿ´Ÿäÿ± ÿßŸÑŸÑÿßŸÇ"
})

