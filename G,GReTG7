TabSp:AddButton({
  Name = "ŸÖÿ≥ŸÉ ÿ¨ŸÖŸäÿπ ÿßŸÑÿßÿ∫ÿ±ÿßÿ∂",
  Callback = function()
    local function equiptool()
  for i,v in ipairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
    if v:IsA("Tool") then
      v.Parent = game.Players.LocalPlayer.Character
    end
  end
end
equiptool()
  end
})

-- ==================== ÿ™ÿ®ŸàŸäÿ®: ÿ≤ÿπÿßÿ∑Ÿäÿ∑ ====================
local ChildTab = Window:MakeTab({
    Title = "ÿ≤ÿπÿßÿ∑Ÿäÿ∑",
    Icon = "baby"
})

-- ÿ™ÿ¨ŸÜÿ® ÿßŸÑÿ™ÿπÿßÿ±ÿ∂ ŸÅŸä ÿßŸÑŸÖÿ™ÿ∫Ÿäÿ±ÿßÿ™
local LocalPlayer = game.Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local Workspace = game.Workspace

local selectedPlayerName = nil

-- ==================== ÿØÿßŸÑÿ© ÿ¨ŸÑÿ® ÿßŸÑŸÑÿßÿπÿ®ŸäŸÜ ====================
local function getPlayerList()
    local list = {}
    for _, p in ipairs(game.Players:GetPlayers()) do
        if p ~= LocalPlayer then
            table.insert(list, p.Name)
        end
    end
    return #list > 0 and list or {"ŸÑÿß ŸäŸàÿ¨ÿØ ŸÑÿßÿπÿ®ŸäŸÜ"}
end

-- ==================== ÿßŸÑŸÇÿßÿ¶ŸÖÿ© ÿßŸÑŸÖŸÜÿ≥ÿØŸÑÿ© ====================
local playerChild = ChildTab:AddDropdown({
    Name = "ÿßÿÆÿ™ÿßÿ± ŸÑÿßÿπÿ®",
    Options = getPlayerList(),
    Default = "ŸÑÿß ŸäŸàÿ¨ÿØ ŸÑÿßÿπÿ®ŸäŸÜ",
    Callback = function(value)
        if value == "ŸÑÿß ŸäŸàÿ¨ÿØ ŸÑÿßÿπÿ®ŸäŸÜ" then
            selectedPlayerName = nil
        else
            selectedPlayerName = value
            getgenv().Target = value
        end
    end
})

-- ==================== ÿ≤ÿ± ÿ™ÿ≠ÿØŸäÿ´ ÿßŸÑŸÇÿßÿ¶ŸÖÿ© ====================
ChildTab:AddButton({
    Name = "ÿ™ÿ≠ÿØŸäÿ´ ÿßŸÑŸÑÿßÿπÿ®ŸäŸÜ",
    Callback = function()
        local newPlayers = getPlayerList()
        playerChild:Set(newPlayers)
        if #newPlayers > 0 then
            playerChild:SetValue(newPlayers[1])
            selectedPlayerName = newPlayers[1]
        else
            playerChild:SetValue("ŸÑÿß ŸäŸàÿ¨ÿØ ŸÑÿßÿπÿ®ŸäŸÜ")
            selectedPlayerName = nil
        end
    end
})

-- ==================== ÿ™ÿ≠ÿØŸäÿ´ ÿ™ŸÑŸÇÿßÿ¶Ÿä ====================
game.Players.PlayerAdded:Connect(function(player)
    task.wait(400)
    local newPlayers = getPlayerList()
    playerChild:Set(newPlayers)
end)

game.Players.PlayerRemoving:Connect(function(player)
    task.wait(400)
    local newPlayers = getPlayerList()
    playerChild:Set(newPlayers)
    if selectedPlayerName == player.Name then
        selectedPlayerName = nil
    end
end)

-- ==================== ÿ≤ÿ±: ÿ•ÿ±ÿ≥ÿßŸÑ ÿßŸÑÿ∑ŸÅŸÑ ====================
ChildTab:AddButton({
    Name = "ÿ•ÿ±ÿ≥ÿßŸÑ ÿßŸÑÿ∑ŸÅŸÑ",
    Callback = function()
        if not selectedPlayerName then
            ChildTab:AddNotification({ Name = "ÿÆÿ∑ÿ£", Content = "ÿßÿÆÿ™ÿ± ŸÑÿßÿπÿ®Ÿãÿß!", Time = 5 })
            return
        end

        local args = { [1] = "CharacterFollowSpawnPlayer", [2] = "BabyBoy" }
        pcall(function()
            ReplicatedStorage.RE:FindFirstChild("1Bab1yFollo1w"):FireServer(unpack(args))
        end)

        task.wait(0.2)

        local char = LocalPlayer.Character
        local target = Workspace:FindFirstChild(selectedPlayerName)
        if target and char and char:FindFirstChild("FollowCharacter") then
            char.FollowCharacter.Parent = target

            if not getgenv().ChildConnection then
                getgenv().ChildConnection = RunService.Heartbeat:Connect(function()
                    local follow = target:FindFirstChild("FollowCharacter")
                    local hrp = target:FindFirstChild("HumanoidRootPart")
                    if follow and hrp and follow.Torso and follow.Torso.BodyPosition then
                        follow.Torso.BodyPosition.Position = hrp.Position - hrp.CFrame.LookVector * 3
                    end
                end)
            end
        end
    end
})

-- ==================== ÿ≤ÿ±: ÿ±ÿ≥ÿ®ŸàŸÜ ÿßŸÑÿ∑ŸÅŸÑ ====================
ChildTab:AddButton({
    Name = "ÿ±ÿ≥ÿ®ŸàŸÜ ÿßŸÑÿ∑ŸÅŸÑ",
    Callback = function()
        if getgenv().ChildConnection then
            getgenv().ChildConnection:Disconnect()
            getgenv().ChildConnection = nil
        end

        pcall(function()
            ReplicatedStorage.RE:FindFirstChild("1Bab1yFollo1w"):FireServer("DeleteFollowCharacter")
            task.wait(0.5)
            ReplicatedStorage.RE:FindFirstChild("1Bab1yFollo1w"):FireServer("CharacterFollowSpawnPlayer", "BabyBoy")
        end)
    end
})

-- ==================== ÿ™ÿ¨ÿ≥ÿ≥ ÿπŸÑŸâ ÿßŸÑŸÑÿßÿπÿ® ====================
ChildTab:AddToggle({
    Name = "ÿ™ÿ¨ÿ≥ÿ≥ ÿπŸÑŸâ ÿßŸÑŸÑÿßÿπÿ®",
    Default = false,
    Callback = function(state)
        local Camera = Workspace.CurrentCamera
        if state then
            if not selectedPlayerName then return end
            if getgenv().SpyConnection then getgenv().SpyConnection:Disconnect() end

            getgenv().SpyConnection = RunService.Heartbeat:Connect(function()
                local target = game.Players:FindFirstChild(selectedPlayerName)
                if target and target.Character then
                    Camera.CameraSubject = target.Character:FindFirstChild("Humanoid") or Camera.CameraSubject
                end
            end)
        else
            if getgenv().SpyConnection then
                getgenv().SpyConnection:Disconnect()
                getgenv().SpyConnection = nil
            end
            Camera.CameraSubject = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") or Camera.CameraSubject
        end
    end
})

