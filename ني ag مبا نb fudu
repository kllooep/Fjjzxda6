            table.insert(itens, item)
        end
    end

    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            table.insert(itens, item)
        end
    end

    -- Ø§Ù„ØªØ­Ù‚Ù‚ Ù…Ù† ÙˆØ¬ÙˆØ¯ Ø£Ø¯ÙˆØ§Øª
    if #itens == 0 then
        warn("Ù„Ø§ ØªÙˆØ¬Ø¯ Ø£Ø¯ÙˆØ§Øª FireX Ù„Ù„ØªÙ†Ø¸ÙŠÙ….")
        return
