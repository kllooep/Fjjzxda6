local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Camera = workspace.CurrentCamera

local Roles = {
    ["Sajwaad"] = "مطور",
    ["ghalidev_0"] = "مطور",   
    ["Zarba_751"] = "ادمن",    
    ["malk_55117"] = "ادمن",
    ["DemonHub_Dev"] = "ادمن",
    ["imane_pink54"] = "ادمن",
    ["zdrkn305"] = "ادمن",
    ["Hwrejplq"] = "ادمن",
    ["Eiles917"] = "ادمن",
    ["mohmmad47n"] = "ادمن",
    ["mr_Mustafa213"] = "ادمن"
}

local tagTracks = {} -- { player = connection }
local tagsEnabled = false -- الحالة الابتدائية

local function createProfessionalTag(player)
    if not tagsEnabled then return end

    local function removeOldTag()
        if player.Character then
            local old = player.Character:FindFirstChild("NameTag")
            if old then
                old:Destroy()
            end
        end
    end
    removeOldTag()

    local function buildTag()
        local character = player.Character
        if not character then return end

        local head = character:FindFirstChild("Head")
        if not head then
            head = character:WaitForChild("Head", 10)
            if not head then return end
        end

        removeOldTag()

        local gui = Instance.new("BillboardGui")
        gui.Name = "NameTag"
        gui.Adornee = head
        gui.Parent = character
        gui.Size = UDim2.new(4, 0, 1.5, 0) -- أطول
        gui.StudsOffset = Vector3.new(0, 3.5, 0)
        gui.AlwaysOnTop = true

        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, 0, 1, 0)
        frame.BackgroundTransparency = 0.1
        frame.BorderSizePixel = 0
        frame.Parent = gui

        local uicorner = Instance.new("UICorner")
        uicorner.CornerRadius = UDim.new(0, 8)
        uicorner.Parent = frame

        local gradient = Instance.new("UIGradient")
        gradient.Rotation = 90
        gradient.Parent = frame

        local stroke = Instance.new("UIStroke")
        stroke.Thickness = 1.5
        stroke.Transparency = NumberSequence.new(0.3)
        stroke.Parent = frame

        local shadow = Instance.new("ImageLabel")
        shadow.Image = "rbxassetid://1316045217"
        shadow.BackgroundTransparency = 1
        shadow.Size = UDim2.new(1, 6, 1, 6)
        shadow.Position = UDim2.new(0, -3, 0, -3)
        shadow.ZIndex = 0
        shadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
        shadow.ImageTransparency = 0.8
        shadow.Parent = frame

        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.Text = "عضو"
        label.TextColor3 = Color3.fromRGB(255, 0, 0)
        label.TextSize = 20
        label.Font = Enum.Font.GothamBold
        label.Parent = frame

        local role = Roles[player.Name] or "عضو"
        label.Text = role

        if role == "مطور" then
            gradient.Color = ColorSequence.new{
                ColorSequenceKeypoint.new(0, Color3.fromRGB(100, 0, 150)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(180, 50, 255))
            }
            stroke.Color = Color3.fromRGB(200, 100, 255)
        elseif role == "ادمن" then
            gradient.Color = ColorSequence.new{
                ColorSequenceKeypoint.new(0, Color3.fromRGB(150, 0, 0)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 50, 50))
            }
            stroke.Color = Color3.fromRGB(255, 100, 100)
        else
            gradient.Color = ColorSequence.new{
                ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 120, 170)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 180, 255))
            }
            stroke.Color = Color3.fromRGB(255, 255, 255)
        end

        spawn(function()
            while gui.Parent do
                if role == "مطور" then
                    stroke.Color = Color3.fromRGB(200, 100, 255)
                elseif role == "ادمن" then
                    stroke.Color = Color3.fromRGB(255, 100, 100)
                else
                    stroke.Color = Color3.fromRGB(255, 255, 255)
                end
                wait(0.5)
            end
        end)
    end

    spawn(buildTag)

    local charConnection = player.CharacterAdded:Connect(function()
        wait(0.5)
        spawn(buildTag)
    end)

    if tagTracks[player] then
        tagTracks[player]:Disconnect()
    end
    tagTracks[player] = charConnection

    player.AncestryChanged:Connect(function(_, parent)
        if not parent then
            if tagTracks[player] then
                tagTracks[player]:Disconnect()
                tagTracks[player] = nil
            end
        end
    end)
end

--// تحديث العلامات
local function updateAllTags()
    for _, player in ipairs(Players:GetPlayers()) do
        -- ✅ تمت إزالة الشرط: player ~= Players.LocalPlayer
        spawn(function()
            createProfessionalTag(player)
        end)
    end
end

--// إزالة جميع العلامات
local function clearAllTags()
    for _, player in ipairs(Players:GetPlayers()) do
        local character = player.Character
        if character then
            local tag = character:FindFirstChild("NameTag")
            if tag then
                tag:Destroy()
            end
        end
    end
end

InfoTab:AddToggle({
    Name = "كشف الرتب",
    Default = true,
    Callback = function(state)
        tagsEnabled = state
        if state then
            updateAllTags()
            game.StarterGui:SetCore("SendNotification", {
                Title = "الرتبة",
                Text = "تم تفعيل كشف الرتبة",
                Duration = 3
            })
        else
            clearAllTags()
            game.StarterGui:SetCore("SendNotification", {
                Title = "الرتبة",
                Text = "تم إيقاف كشف الرتبة",
                Duration = 3
            })
        end
    end
})

--// تفعيل تلقائي عند تشغيل السكريبت
spawn(function()
    wait(1)
    tagsEnabled = true
    updateAllTags() -- ✅ سيتم تضمينك أنت أيضًا الآن
end)

--// على دخول لاعب جديد
Players.PlayerAdded:Connect(function(player)
    if tagsEnabled then
        spawn(function()
            createProfessionalTag(player)
        end)
    end
end)

local Tab = Window:MakeTab({ 
Title = "الأسماء", 
Icon = "paper" })

local speeds = {
    [1] = 0.4,   
    [2] = 0.25,  
    [3] = 0.15, 
    [4] = 0.08,  
    [5] = 0.04  
}

local currentSpeed = speeds[2] -- القيمة الافتراضية: عادية

Tab:AddSlider({
    Name = "سرعة التلوين",
    Min = 1,
    Max = 5,
    Increment = 1,
    Default = 2, -- الافتراضي: عادية
    ValueName = "Speed",
    Callback = function(Value)
        currentSpeed = speeds[Value]
    end
})

-- Toggle لتلوين البايو
local bioToggleActive = false
Tab:AddToggle({
    Name = "بايو",
    Default = false,
    Callback = function(Value)
        bioToggleActive = Value
        if not Value then
            local args = {"PickingRPBioColor", Color3.fromRGB(255,255,255)}
            game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1RPNam1eColo1r"):FireServer(unpack(args))
        else
            spawn(function()
                local hue = 0
                while bioToggleActive do
                    local color = Color3.fromHSV(hue, 1, 1)
                    local args = {"PickingRPBioColor", color}
                    game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1RPNam1eColo1r"):FireServer(unpack(args))
                    hue = hue + 0.002
                    if hue > 1 then hue = 0 end
                    task.wait(currentSpeed)
                end
            end)
        end
    end
})

-- Toggle لتلوين الاسم
local nameToggleActive = false
Tab:AddToggle({
    Name = "الاسم",
    Default = false,
    Callback = function(Value)
        nameToggleActive = Value
        if not Value then
            local args = {"PickingRPNameColor", Color3.fromRGB(255,255,255)}
            game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1RPNam1eColo1r"):FireServer(unpack(args))
        else
            spawn(function()
                local hue = 0
                while nameToggleActive do
                    local color = Color3.fromHSV(hue, 1, 1)
                    local args = {"PickingRPNameColor", color}
                    game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1RPNam1eColo1r"):FireServer(unpack(args))
                    hue = hue + 0.002
                    if hue > 1 then hue = 0 end
                    task.wait(currentSpeed)
                end
            end)
        end
    end
})

local SectionSpam = Tab:AddSection({
    Name = "(new)سبام اسماء"
})


-- متغيرات
local names = {"", "", ""}
local execSpeed = 1
local toggleActive = false

-- إنشاء TextBox للأسماء الثلاثة
for i = 1, 3 do
Tab:AddTextBox({
        Name = "أدخل الاسم " .. i,
        Default = "",
        TextDisappear = false,
        Callback = function(value)
            names[i] = value
        end
    })
end

-- Slider لتحديد السرعة
Tab:AddSlider({
    Name = "سرعة السبام",
    Min = 1,
    Max = 3,
    Default = 1,
    Increment = 1,
    ValueName = "x",
    Callback = function(value)
        execSpeed = value
    end
})

-- Toggle للتشغيل مع الحماية
Tab:AddToggle({
    Name = "تشغيل السبام",
    Default = false,
    Callback = function(state)
        toggleActive = state
        if toggleActive then
            -- تحقق أن فيه على الأقل اسم واحد مكتوب
            local hasName = false
            for _, name in ipairs(names) do
                if name ~= "" then
                    hasName = true
                    break
                end
            end

            if not hasName then
                game:GetService("StarterGui"):SetCore("SendNotification", {
                    Title = "تحذير",
                    Text = "يجب إدخال اسم واحد على الأقل قبل التشغيل"
                })
                toggleActive = false
                return
            end

            -- بدء التشغيل
            task.spawn(function()
                while toggleActive do
                    for _, name in ipairs(names) do
                        if not toggleActive then break end
                        if name ~= "" then
                            local args = {
                                "RolePlayName",
                                name
                            }
                            game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1RPNam1eTex1t"):FireServer(unpack(args))
                            task.wait((4 - execSpeed) * 0.5)
                        end
                    end
                end
            end)
        end
    end
})

