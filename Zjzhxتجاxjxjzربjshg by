-- ==================== إنشاء تاب "الاكسسوارات" ====================
local BodyTab = Window:MakeTab({
    Title = "الأجسام والرؤوس",
    Icon = "shirt"
})

-- دالة مساعدة لارتداء عنصر
local function Wear(ItemId)
    local args = { [1] = ItemId }
    game:GetService("ReplicatedStorage"):WaitForChild("Remotes", 9e9):WaitForChild("Wear", 9e9):InvokeServer(unpack(args))
end

-- ==================== 1. قسم: ملابس ====================
BodyTab:AddSection({ Name = "الرؤوس" })

local HeadsOptions = {
    "رأس روبـوت",
    "رأس مـخـفـي",
    "رأس كـوبـي| رول",
    "رأس عـيـون زرقـاء"
}

local HeadsIds = {
    3210773801,
134082579,
746767604,
16580493236
}

local selectedHeads = HeadsOptions[1]

BodyTab:AddDropdown({
    Name = "اختر الرأس",
    Options = HeadsOptions,
    Default = selectedHeads,
    Callback = function(value)
        selectedHeads = value
    end
})

BodyTab:AddButton({
    Name = "تفعيل الرأس",
    Callback = function()
        for i, name in ipairs(HeadsOptions) do
            if name == selectedHeads then
                Wear(HeadsIds[i])
                break
            end
        end
    end
})

-- دالة مساعدة لتغيير الجسم
local function ChangeBody(bodyArgs)
    local args = { bodyArgs }
    game:GetService("ReplicatedStorage").Remotes.ChangeCharacterBody:InvokeServer(unpack(args))
end

-- ==================== 1. قسم: أجسام البنات ====================
BodyTab:AddSection({ Name = "أجسام البنات" })

-- خيارات أجسام البنات
local girlBodyOptions = {
    "جسم بنت رول",
    "جسم بنت يد طويلة",
    "جسم بنت خصر",
    "جسم بنت خصر2",
    "جسم بنت بكمية"
}

-- القيم (Body IDs) لكل جسم
local girlBodyValues = {
    {96491916349570, 76683091425509, 75159926897589, 1, 1, 1},
    {96491916349570, 14854350570, 14854350451, 1, 1, 1},
    {74302534603111, 76683091425509, 75159926897589, 1, 1, 1},
    {18839824113, 18839824209, 18839824132, 1, 1, 1},
    {15539008532, 15539008875, 15539008680, 15539008795, 15539011945, 1}
}

-- المتغير المختار
local selectedGirlBody = girlBodyOptions[1]

-- القائمة المنسدلة
BodyTab:AddDropdown({
    Name = "اختر جسم بنت",
    Options = girlBodyOptions,
    Default = selectedGirlBody,
    Callback = function(value)
        selectedGirlBody = value
    end
})

-- زر التفعيل
BodyTab:AddButton({
    Name = "تفعيل جسم البنت",
    Callback = function()
        for i, name in ipairs(girlBodyOptions) do
            if name == selectedGirlBody then
                ChangeBody(girlBodyValues[i])
                break
            end
        end
    end
})



